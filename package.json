{
  "name": "util-helpers",
  "version": "1.0.0",
  "description": "一个基于业务场景的工具方法库",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx babel src --out-dir lib",
    "build:source-maps": "npx babel src --out-dir lib --source-maps inline",
    "doc": "npx jsdoc src/ README.md -d docs -r",
    "pub": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doly-dev/util-helpers.git"
  },
  "keywords": [
    "util",
    "bs-util",
    "util-helpers",
    "bs-helpers"
  ],
  "author": "caijf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doly-dev/util-helpers/issues"
  },
  "homepage": "https://doly-dev.github.io/util-helpers/index.html",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-umd": "^7.2.0",
    "@babel/preset-env": "^7.4.5"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "browsers": [
              "> 1%",
              "last 4 versions",
              "Firefox ESR",
              "not ie < 9"
            ]
          },
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-modules-umd"
    ]
  }
}
